{
  "name": "darkwebb-solidity",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/webb-tools/darkwebb-solidity.git",
  "author": "Webb Technologies",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "compile": "npx hardhat compile",
    "build": "yarn compile && yarn build:circuits && yarn build:hashers && yarn build:ptau",
    "build:circuits": "./scripts/bash/compile_circom.sh",
    "build:hashers": "ts-node scripts/ts/build_hashers.ts",
    "build:ptau": "./scripts/bash/generate_phase1_ptau.sh",
    "build:contracts:types": "typechain --target ethers-v5 --out './typechain' './artifacts/**/*.json'",
    "run:deploy-webb": "ts-node ./scripts/evm/webbSystemDeployment.ts",
    "run:inspect-webb": "ts-node ./scripts/evm/inspectWebbInstance.ts",
    "run:link-webbs": "ts-node ./scripts/evm/linkAnchors.ts",
    "setup:poseidon-bridge2": "node scripts/js/generatePoseidonBridge2Input.js && ./scripts/bash/phase2_poseidon_bridge2.sh",
    "setup:poseidon-bridge3": "node scripts/js/generatePoseidonBridge3Input.js && ./scripts/bash/phase2_poseidon_bridge3.sh",
    "setup:poseidon-bridge4": "node scripts/js/generatePoseidonBridge4Input.js && ./scripts/bash/phase2_poseidon_bridge4.sh",
    "setup:poseidon-bridge5": "node scripts/js/generatePoseidonBridge5Input.js && ./scripts/bash/phase2_poseidon_bridge5.sh",
    "setup:poseidon-bridge6": "node scripts/js/generatePoseidonBridge6Input.js && ./scripts/bash/phase2_poseidon_bridge6.sh",
    "setup:all-bridges": "yarn setup:poseidon-bridge2 && yarn setup:poseidon-bridge3 && yarn setup:poseidon-bridge4 && yarn setup:poseidon-bridge5 && yarn setup:poseidon-bridge6",
    "setup:poseidon2-preimage": "node scripts/js/generatePoseidon2Input.js && ./scripts/bash/phase2_poseidon.sh",
    "setup:poseidon3-preimage": "node scripts/js/generatePoseidon3Input.js && ./scripts/bash/phase2_poseidon3.sh",
    "setup": "yarn setup:poseidon-bridge2 && yarn setup:poseidon2-preimage && yarn setup:poseidon3-preimage",
    "test:all": "yarn build && yarn setup && yarn test",
    "test:bridge": "yarn build && yarn setup:poseidon-bridge2 && yarn test",
    "test": "yarn build:hashers && npx hardhat test"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.3.1",
    "ffiasm": "^0.1.1",
    "ffjavascript": "^0.2.38",
    "hardhat": "^2.4.1",
    "snarkjs": "^0.4.6",
    "solparse": "^2.2.8",
    "truffle-assertions": "^0.9.2",
    "web3": "^1.3.6",
    "web3-utils": "^1.2.11"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@truffle/contract": "^4.3.24",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "big-integer": "^1.6.48",
    "bn-chai": "^1.0.1",
    "chai-as-promised": "^7.1.1",
    "circom": "^0.5.45",
    "circom_runtime": "^0.1.9",
    "circomlib": "^0.5.2",
    "dotenv": "^8.2.0",
    "ethers-eip712": "^0.2.0",
    "ganache-cli": "^6.12.2",
    "hardhat-artifactor": "^0.2.0",
    "hardhat-circom": "^1.0.1",
    "maci-crypto": "^0.9.1",
    "maci-domainobjs": "^0.9.1",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2"
  }
}
